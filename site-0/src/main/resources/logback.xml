<configuration>
    <appender name="STDOUT" class="ch.qos.logback.core.ConsoleAppender">
        <encoder>
            <pattern>%d{HH:mm:ss.SSS} [%thread] %-5level %logger{36} - %msg%n</pattern>
        </encoder>
    </appender>

    <logger name="com.yih.chasm" level="INFO">
        <appender-ref ref="STDOUT" />
    </logger>

    <logger name="com.yih.chams.sql" level="DEBUG">
        <appender-ref ref="STDOUT" />
    </logger>

    <!--<root level="DEBUG">-->
        <!--<appender-ref ref="STDOUT" />-->
    <!--</root>-->
</configuration>